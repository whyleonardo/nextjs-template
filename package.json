{
  "name": "next-template",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rimraf --glob node_modules dist .next bun.lockb .tsbuildinfo test-results test-coverage-results",
    "copy-env": "cp .env.example .env",
    "dev": "next dev --turbo",
    "format": "prettier --write --ignore-unknown \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier \"**/*\" --ignore-unknown --list-different",
    "format:write": "prettier \"**/*\" --ignore-unknown --list-different --write",
    "lint": "biome lint --write ./src/**",
    "prepare": "husky",
    "preview": "next build && next start",
    "start": "next start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "ui:add": "bun x shadcn-ui@latest add"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,cjs,mjs,json}": [
      "biome lint --apply ."
    ],
    "*": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@biomejs/biome": "1.7.3",
    "@radix-ui/react-slot": "1.0.2",
    "@t3-oss/env-nextjs": "0.10.0",
    "babel-plugin-react-compiler": "0.0.0-experimental-487cb0e-20240529",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "geist": "1.3.0",
    "lucide-react": "0.381.0",
    "next": "15.0.0-rc.0",
    "next-themes": "0.3.0",
    "react": "19.0.0-rc-935180c7e0-20240524",
    "react-dom": "19.0.0-rc-935180c7e0-20240524",
    "sharp": "0.33.3",
    "tailwind-merge": "2.2.2",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@tailwindcss/typography": "0.5.13",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.12.7",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@vitejs/plugin-react": "4.3.1",
    "@vitest/coverage-v8": "2.0.3",
    "@vitest/ui": "2.0.3",
    "autoprefixer": "10.4.19",
    "fluid-tailwind": "0.3.8",
    "husky": "9.0.11",
    "jsdom": "24.1.0",
    "lint-staged": "15.2.5",
    "mini-svg-data-uri": "1.4.4",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "2.5.0",
    "prettier-plugin-tailwindcss": "0.6.0",
    "rimraf": "5.0.7",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.4",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.3",
    "vitest-fail-on-console": "0.7.0"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
